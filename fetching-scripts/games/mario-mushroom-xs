#!/usr/bin/env bash

f=3 b=4

for j in f b; do
	for i in {0..7}; do
		printf -v $j$i %b "\e[${!j}${i}m"
	done
done

for i in {0..7}; do
		printf -v g$i %b "\e[9${i}m"
done

